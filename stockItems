package com.rit.inherit;

public class StockItems {
	
	int id;
	String name;
	int price;
	
	public StockItems() {
		// TODO Auto-generated constructor stub
	}
	
	/**
	 * @param id
	 * @param name
	 * @param price
	 */
	StockItems(int id, String name, int price) {
		this.id = id;
		this.name = name;
		this.price = price;
	}
	
}
